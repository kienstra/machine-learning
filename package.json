  "keywords": [
    "WordPress",
    "plugin",
    "survey"
  ],
  "homepage": "https://github.com/kienstra/easy-survey#readme",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/kienstra/easy-survey.git"
  },
